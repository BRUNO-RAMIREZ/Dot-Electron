{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "5710981c-b162-456d-8c75-e694eff956ee"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "dot-electron": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "baseHref": "/",
            "deployUrl": "/",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@doodle/viewer/assets/",
                "output": "./assets/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@doodle/annotations/assets/",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@sdv/sdv-components/assets/images",
                "output": "./assets/images"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@doodle/extension/assets/",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@set-social-services/comment-view/images/",
                "output": "./assets/comment-view/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@set-social-services/comment-view/sounds/",
                "output": "./assets/comment-view/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@set-social-services/micro-app-view/images/",
                "output": "./assets/micro-app-view/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@set-social-services/wall-view/images/",
                "output": "./assets/wall-view/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@set-social-services/wall-dobar/images/",
                "output": "./assets/wall-dobar/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@set/action-tags-custom-components/assets",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@set/action-tags-ng-plugin/assets",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@set/reference-tags-ng-plugin/assets",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@set/reference-tags-custom-components/assets",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@set/social-tags-commons/assets",
                "output": "/assets/"
              },
              {
                "glob": "*",
                "input": "./node_modules/@set/topic-tags-ng-plugin/assets/images",
                "output": "assets/images/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@set/user-tag-ng-plugin/assets",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@doodle/thumbnail/dist/assets/",
                "output": "./assets/"
              }
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/@doodle/annotations/style.css",
              "node_modules/@doodle/extension/assets/dd-styles.scss",
              "node_modules/@doodle/viewer/assets/styles/doodle.viewer.min.css",
              "node_modules/@doodle/thumbnail-js/style.css",
              "@ctrl/ngx-emoji-mart/picker.css",
              "node_modules/@sdv/sdv-components/assets/styles/sdv.components.min.css",
              "node_modules/@set-social-services/common-style/common.style.min.css",
              "node_modules/@set-social-services/common-component/styles/common.component.min.css",
              "node_modules/@set-social-services/micro-app-view/styles/micro.app.view.min.css",
              "node_modules/@set-social-services/comment-view/styles/comment.view.min.css",
              "node_modules/@set-social-services/comments-plugin/styles/comments.plugin.min.css",
              "node_modules/@set-social-services/wall-view/styles/wall.view.min.css",
              "node_modules/@set/topic-tags-ng-plugin/styles/topic-tags.css",
              "node_modules/@set/user-tag-ng-plugin/styles/user-tags-styles.scss",
              "node_modules/@set/ati-tags-ng-plugin/assets/styles/ati-tag-styles.scss",
              "node_modules/@set/action-tags-ng-plugin/assets/styles/action-tag-styles.scss",
              "node_modules/@set/reference-tags-ng-plugin/assets/styles/reference-tag-styles.scss",
              "node_modules/@set/social-tags-commons/assets/styles/social-tags-common-styles.scss",
              "node_modules/@set/web-tags-ng-plugin/styles/web-tags-styles.scss",
              "node_modules/@set/reference-tags-custom-components/assets/styles/references-tags-custom-styles.scss"
            ],
            "scripts": [
              "node_modules/@doodle/viewer/assets/js/dd-font-family-kit.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "50mb",
                  "maximumError": "70mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "dot-electron:build:production"
            },
            "development": {
              "browserTarget": "dot-electron:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dot-electron:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "dot-electron"
}
